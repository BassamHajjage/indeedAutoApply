{
  "manifest_version": 3,
  "name": "Indeed Auto Apply",
  "description": "Automatically apply to Indeed job listings",
  "version": "0.1",
  "author": "Zebulun McNeill",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ]
}
